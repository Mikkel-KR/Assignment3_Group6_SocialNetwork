@{
    ViewData["Title"] = "All Registered Users";
}

@model List<User>

<table class="table table-striped">
    <thead>
        <tr>
            <th>User Id</th>
            <th>Name</th>
            <th>Age</th>
            <th>Gender</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Id</td>
                <td>@user.UserName</td>
                <td>@user.Age</td>
                <td>@user.Gender</td>
            </tr>
        }
    </tbody>
</table>